{
  "name": "amazon-checkout-automation",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Chanpreet Singh <chanpreet3000@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start:windows": "concurrently \"yarn run start:backend\" \"yarn run start:frontend\"",
    "start:frontend": "cd frontend && yarn start",
    "start:backend": "cd backend && .\\.venv\\Scripts\\activate && uvicorn main:app --reload",
    "start:mac": "concurrently \"yarn run start:mac-backend\" \"yarn run start:maac-frontend\"",
    "start:mac-frontend": "cd frontend && yarn start",
    "start:mac-backend": "cd backend && source .venv/bin/activate && uvicorn main:app --reload"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
